  <rdf:Description rdf:about="http://dbpedia.org/resource/Bánh_hỏi">
    <dbo:wikiPageWikiLink rdf:resource="http://dbpedia.org/resource/Category:Street_food_in_Vietnam"/>
    <dbo:wikiPageWikiLink rdf:resource="http://dbpedia.org/resource/Category:Vietnamese_words_and_phrases"/>
